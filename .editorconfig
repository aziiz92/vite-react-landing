root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[{package.json,package-lock.json}]
indent_style = space
indent_size = 2

[*.{ts,tsx,js,jsx,json,css,html,md}]
indent_style = space
indent_size = 2

